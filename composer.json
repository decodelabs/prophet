{
    "name": "decodelabs/prophet",
    "description": "Generalised interfaces for creating AI assistants",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.4",
        "decodelabs/atlas": "^0.14",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/dictum": "^0.7",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/kingdom": "^0.1",
        "decodelabs/monarch": "^0.2",
        "decodelabs/slingshot": "^0.3",

        "nesbot/carbon": "^3"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Prophet\\": "src/Prophet/"
        },
        "classmap": [
            "src/Prophet.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
